{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type":"array",
    "items": {
        "type": "object",
        "properties": {
            "company": {
                "type": "string"
            },
            "position": {
                "type": "string"
            },
            "imageUrl": {
                "type": "string"
            },
            "start": {
                "type": "string"
            },
            "end": {
                "type": "string"
            },
            "projects": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "projectName": {
                            "type": "string"
                        },
                        "imageUrl": {
                            "type": "string"
                        },
                        "description": {
                            "type": "string"
                        },
                        "technologies": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "technologyName": {
                                        "type": "string"
                                    },
                                    "iconClass": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "required": ["projectName", "description"]
                }
            }
        }
    }
}